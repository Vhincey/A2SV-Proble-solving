class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        x_string = str(x)

        if x_string == x_string[::-1]:
            return True
        else:
            return False

myObject = Solution()

print(myObject.isPalindrome(124))
   


        
        
