    # def q(self, t: int) -> int:
    #     n = len(self.persons)
    #     l, r = 0, n - 1

    #     while l < r:
    #         mid = l + (r - l) // 2

    #         if t < self.times[mid] :
    #             r = mid - 1
    #         elif t > self.times[mid]:
    #             l = mid + 1
    #         else:
    #             return self.persons[mid]
    #     if t > self.times[-1]:
    #       return self.max_count
    #     else:
    #       return self.persons[l - 1]


    def __init__(self, persons: List[int], times: List[int]):
        self.persons = persons
        self.times = times
        self.leader_counts = defaultdict(int)
        self.leaders = []
        max_person = None
        max_count = 0

        for person in persons:
            self.leader_counts[person] += 1
            if self.leader_counts[person] >= max_count:
                max_person = person
                max_count = self.leader_counts[person]
            self.leaders.append(max_person)

    def q(self, t: int) -> int:
      n = len(self.times)
      l, r = 0, n - 1

      while l < r:
          mid = l + (r - l + 1) // 2
          
          if self.times[mid] <= t:
            l = mid
          else:
            r = mid - 1

      return self.leaders[l]


# Your TopVotedCandidate object will be instantiated and called as such:
# obj = TopVotedCandidate(persons, times)
# param_1 = obj.q(t)
